{
  "expo": {
    "name": "expo-love-bites",
    "slug": "expo-love-bites",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ghazwairshad.lovebitesgoogle",
      "config": {
        "googleMapsApiKey": "AIzaSyAhMfwYOoPWPwsGtHCfU5XfnsJrMjjRl8A"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ghazwairshad.lovebitesgoogle",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAhMfwYOoPWPwsGtHCfU5XfnsJrMjjRl8A"
        }
      },
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.478147632680-du93puo7pbkdjmdtrahhqergbu1l5plq"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $Love Bites to use your location."
        }
      ],
    ],
    "extra": {
      "eas": {
        "projectId": "0ba1e25d-a00b-4196-beba-44825fec6885"
      }
    },
    "scheme": "expo-love-bites"
  }
}
